{
    "name": "smart-cms/core",
    "description": "This is my package core",
    "keywords": [
        "smart_cms",
        "laravel",
        "cms",
        "filament",
        "core"
    ],
    "homepage": "https://github.com/smart-cms/core",
    "license": "MIT",
    "authors": [
        {
            "name": "smart_cms",
            "email": "maxboiko@divotek.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "bezhansalleh/filament-language-switch": "^3.1",
        "filament/filament": "^3.3",
        "gregwar/image": "^2.1",
        "guava/filament-icon-picker": "^2.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "intervention/image": "^2.7",
        "joshembling/image-optimizer": "^1.4",
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.4",
        "lorisleiva/laravel-actions": "^2.8",
        "outerweb/filament-settings": "*",
        "outerweb/settings": "^1.0",
        "saade/filament-adjacency-list": "^3.2",
        "schmeits/filament-character-counter": "^1.3",
        "spatie/laravel-package-tools": "^1.16",
        "symfony/yaml": "^7.1",
        "ysfkaya/filament-phone-input": "*"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.34",
        "pestphp/pest-plugin-arch": "^2.7",
        "pestphp/pest-plugin-laravel": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "SmartCms\\Core\\": "src/"
        },
        "files": [
            "src/helpers/base.php",
            "src/helpers/core.php",
            "src/helpers/lang.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SmartCms\\Core\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "",
        "build": [
            ""
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "format": "vendor/bin/pint",
        "test": "vendor/bin/pest"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "pestphp/pest-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SmartCms\\Core\\SmartCmsServiceProvider",
                "SmartCms\\Core\\SmartCmsPanelManager"
            ],
            "aliases": {
                "Core": "SmartCms\\Core\\Facades\\Core"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
